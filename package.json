{
  "name": "voxdem-survey-api",
  "version": "1.0.0",
  "description": "API for survey data analysis with PostgreSQL and TypeORM",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:setup": "ts-node src/scripts/setup-database.ts",
    "db:test": "ts-node src/scripts/test-connection.ts",
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:import": "ts-node src/scripts/import-real-data.ts",
    "db:reset": "npm run db:setup && npm run db:test && npm run db:import",
    "process-questions": "npm run build && node dist/scripts/process-questions.js",
    "import-data": "npm run build && node dist/scripts/import-data.js",
    "import-direct": "npm run build && node dist/scripts/import-direct.js",
    "execute-import": "npm run build && node dist/scripts/execute-import.js",
    "csv-to-db": "npm run build && node dist/scripts/csv-to-database.js",
    "db:check": "npm run build && node dist/scripts/check-database.js",
    "db:create-new": "echo 'Execute os scripts SQL na ordem: 01_create_tables.sql, 02_populate_lookup_tables.sql, depois npm run process-questions e npm run import-data'"
  },
  "keywords": [
    "typescript",
    "postgresql",
    "typeorm",
    "survey",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
